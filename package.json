{
  "name": "chinese_regions",
  "version": "0.5.1",
  "description": "中华人民共和国民政部省、市、区/县级行政区划代码及名称。港澳台地区等均已包含。当前数据最后更新于2025年5月14日，根据中华人民共和国民政部于2024年04月25日最新发布的数据生成。",
  "keywords": [
    "省市区",
    "行政区",
    "中国",
    "中华人民共和国",
    "prc",
    "china",
    "regions",
    "province",
    "city",
    "district",
    "county",
    "taiwan",
    "hongkong",
    "macao"
  ],
  "author": "Lau, Kamas <kamaslau@dingtalk.com> (https://www.kamaslau.com/)",
  "license": "MIT",
  "type": "module",
  "main": "out/all.min.json",
  "engines": {
    "node": "^24 || >=22"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env.local dist/app.js",
    "start:dev": "node --env-file=.env.local --watch=.env.local --watch=pnpm-lock.yaml --watch=tsconfig.json --watch-path=./src --watch-path=./types --import=tsx src/app.ts",
    "test": "node --test",
    "lint": "ts-standard --fix src/**/*.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/koa": "^3.0.1",
    "@types/node": "^25.0.3",
    "cheerio": "1.1.2",
    "husky": "^9.1.7",
    "ts-standard": "^12.0.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamaslau/chinese_regions.git"
  },
  "bugs": {
    "url": "https://github.com/kamaslau/chinese_regions/issues"
  },
  "homepage": "https://github.com/kamaslau/chinese_regions#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@koa/router": "^15.1.1",
    "fs-extra": "^11.3.3",
    "koa": "^3.1.1"
  }
}
